    <div class="limiter">
        <div class="container-login100" style="background-image: url('images/bg-01.jpg');">
            <div class="wrap-login100 p-l-55 p-r-55 p-t-15 p-b-54">

                <form action="@Url.Action("Register","Home")" method="post" class="login100-form validate-form">
                    <span class="login100-form-title p-b-49">
                        Üye Ol
                    </span>
                    <div class="wrap-input100 validate-input m-b-23" data-validate="Ad Soyad Zorunlu Alan">
                        <span class="label-input100">Ad Soyad</span>
                        <input class="input100" type="text" asp-for="NameSurname" placeholder="Ad Soyad" maxlength="100">
                        <span class="focus-input100" data-symbol="&#xf206;"></span>
                    </div>
                    <div class="wrap-input100 validate-input m-b-23" data-validate="Kullanıcı Adı Zorunlu Alan">
                        <span class="label-input100">Kullanıcı Adı</span>
                        <input class="input100" type="text" asp-for="UserName" placeholder="Kullanıcı Adı" maxlength="100">
                        <span class="focus-input100" data-symbol="&#xf206;"></span>
                    </div>
                    <div class="wrap-input100 validate-input m-b-23" data-validate="Email Zorunlu Alan">
                        <span class="label-input100">Email</span>
                        <input class="input100" type="text" asp-for="Email" placeholder="Email" maxlength="100">
                        <span class="focus-input100" data-symbol="&#xf206;"></span>
                    </div>
                    
                    <div class="wrap-input100 validate-input" data-validate="Şifre Zorunlu Alan">
                        <span class="label-input100">Şifre</span>
                        <input class="input100" type="password" asp-for="Password" placeholder="Şifre" maxlength="100">
                        <span class="focus-input100" data-symbol="&#xf190;"></span>
                    </div>
                    <div class="wrap-input100 validate-input m-b-23" data-validate="Telefon Zorunlu Alan">
                        <span class="label-input100">Telefon</span>
                        <input class="input100" type="text" asp-for="Telephone" placeholder="Telefon" maxlength="100">
                        <span class="focus-input100" data-symbol="&#xf2b6;"></span>
                    </div>
                    <div class="wrap-input100 validate-input m-b-23" data-validate="Adres Zorunlu Alan">
                        <span class="label-input100">Adres</span>
                        <input class="input100" type="text" asp-for="InvoicesAddres" placeholder="Adres" maxlength="100">
                        <span class="focus-input100" data-symbol="&#xf158;"></span>
                    </div>
                    <br />
                    <div class="container-login100-form-btn">
                        <div class="wrap-login100-form-btn">
                            <div class="login100-form-bgbtn"></div>
                            <button class="login100-form-btn">
                                Üye Ol
                            </button>
                        </div>
                    </div>


                </form>
            </div>
        </div>
    </div>
    <div id="dropDownSelect1"></div>

